                                    <td>20%</td>
                                    <td>120</td>
                                    <td>
                                        <i class="bi bi-pencil-square"></i>
                                    </td>
                                    <td>
                                        <i class="bi bi-trash-fill"></i>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>

                         
            </div>
        </div>
    </div>

<%- include("../../end.ejs") %>